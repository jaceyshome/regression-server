{
  "name": "regression-test-server",
  "version": "1.8.2",
  "description": "Regression test server, RESTful API application with Koa.js, Docker, Swagger, Jest, Coveralls, and Circle CI",
  "author": "Jake Wang",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/jaceyshome/regression-test-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://jaceyshome@bitbucket.org/jaceyshome/regression-test-server.git"
  },
  "keywords": [
    "koa",
    "koa2",
    "rest",
    "restful",
    "api",
    "regression",
    "skeleton"
  ],
  "engines": {
    "node": ">= 7.10.0"
  },
  "main": "./web/index.js",
  "scripts": {
    "start": "node ./web/index.js",
    "dev": "NODE_ENV=development nodemon start ./web/index.js --quiet | bunyan",
    "test": "NODE_ENV=test jest --forceExit --verbose",
    "test:unit": "npm test -- test/unit",
    "test:integration": "npm test -- test/integration",
    "test:watch": "npm test -- --watch",
    "test:watch:unit": "npm run test:unit -- --watch",
    "test:watch:integration": "npm run test:integration -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:coverage:unit": "npm run test:unit -- --coverage",
    "test:coverage:integration": "npm run test:integration -- --coverage",
    "lint": "eslint app test",
    "lint:app": "eslint app",
    "lint:test": "eslint test"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "dotenv": "^5.0.0",
    "elastic-apm-node": "^1.0.0",
    "factory_girl": "^0.2.3",
    "fs-extra": "^5.0.0",
    "joi": "^13.1.2",
    "kcors": "^2.2.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.5",
    "lorem-ipsum": "^1.0.4",
    "moment": "^2.21.0",
    "nedb": "^1.8.0",
    "pm2": "^2.9.3",
    "raw-body": "^2.3.2",
    "swagger-jsdoc": "^1.9.7",
    "useragent": "^2.3.0",
    "uuid": "^3.2.1",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.17.0",
    "eslint-config-omnious": "^0.3.1",
    "jest": "^22.1.4",
    "jest-junit": "^3.5.0",
    "nodemon": "^1.14.12",
    "supertest": "^3.0.0"
  }
}
